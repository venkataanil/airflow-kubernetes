{
    "benchmarks": [
        {
            "name": "scale-24",
            "workload": "scale-perf",
            "command": "./run_scale_fromgit.sh",
            "env": {
                "SCALE": "24",
                "METADATA_COLLECTION": "true",
                "WORKLOAD_NODE_ROLE": "worker"
            }
        },
        {
            "name": "node-density",
            "workload": "kube-burner",
            "custom_cmd": "kube-burner ocp node-density --uuid=${UUID} --pods-per-node=245 --timeout=3h --pod-ready-threshold=20s --es-server=${ES_SERVER} --es-index=ripsaw-kube-burner --user-metadata=metadata.yml"
        },
        {
            "name": "node-density-sleep",
            "workload": "kube-burner",
            "custom_cmd": "sleep 30m"
        },
        {
            "name": "node-density-cni",
            "workload": "kube-burner",
            "custom_cmd": "kube-burner ocp node-density-cni --uuid=${UUID} --pods-per-node=120 --timeout=8h --es-server=${ES_SERVER} --es-index=ripsaw-kube-burner --user-metadata=metadata.yml"
        },
        {
            "name": "node-density-cni-sleep",
            "workload": "kube-burner",
            "custom_cmd": "sleep 30m"
        },
        {
            "name": "cluster-density-v2",
            "workload": "kube-burner",
            "custom_cmd": "kube-burner ocp cluster-density-v2 --uuid=${UUID} --iterations=500 --churn-delay=10m --churn-percent=10 --timeout=6h --es-server=${ES_SERVER} --es-index=ripsaw-kube-burner --user-metadata=metadata.yml --gc=false"
        }
    ]
}
