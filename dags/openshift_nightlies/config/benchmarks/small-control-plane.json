{
    "benchmarks": [
        {
            "name": "scale-24",
            "workload": "scale-perf",
            "command": "./run_scale_fromgit.sh",
            "env": {
                "SCALE": "24",
                "METADATA_COLLECTION": "true",
                "WORKLOAD_NODE_ROLE": "worker"
            }
        },
        {
            "name": "node-density-run1",
            "workload": "kube-burner",
            "custom_cmd": "kube-burner ocp node-density --uuid=${UUID} --pods-per-node=245 --timeout=2h --pod-ready-threshold=15m --es-server=${ES_SERVER} --es-index=ripsaw-kube-burner --user-metadata=metadata.yml"
        },
        {
            "name": "node-density-sleep1",
            "workload": "kube-burner",
            "custom_cmd": "sleep 30m"
        },
